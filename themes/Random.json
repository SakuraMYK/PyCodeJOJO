{
  "name": "Random",
  "type": "dark",
  "colors": {
    "editor.background": "#3c6e01",
    "editor.foreground": "#285be5",
    "activityBarBadge.background": "#f5ef3c",
    "sideBarTitle.foreground": "#7810b2",
    "statusBar.background": "#cc77d1",
    "statusBar.foreground": "#f5aa6b",
    "statusBar.border": "#2ef615",
    "statusBar.debuggingBackground": "#23e11f",
    "statusBar.debuggingForeground": "#8e04f4",
    "statusBar.noFolderBackground": "#5d30b7",
    "statusBarItem.activeBackground": "#e417e7",
    "statusBarItem.hoverBackground": "#2313bf",
    "titleBar.activeBackground": "#3a9509",
    "titleBar.activeForeground": "#4c6c5a",
    "titleBar.inactiveBackground": "#e9a470",
    "titleBar.inactiveForeground": "#a0b59b",
    "titleBar.border": "#f2e162",
    "activityBar.background": "#f8a42e",
    "activityBar.foreground": "#5f5b10",
    "activityBar.border": "#047f6b",
    "sideBar.background": "#735d79",
    "sideBar.foreground": "#661e47",
    "sideBar.border": "#eb580d",
    "selection.background": "#FF174455",
    "editor.selectionBackground": "#ae354d33",
    "editor.wordHighlightBackground": "#a2181878",
    "editor.lineHighlightBackground": "#3c3b4e37",
    "editorCursor.foreground": "#c51fc3",
    "editorLineNumber.foreground": "#ed39ca",
    "editorLineNumber.activeForeground": "#d93b3f",
    "tab.activeBackground": "#0e5dac",
    "tab.activeForeground": "#b357e2",
    "tab.activeBorder": "#5f82b1",
    "tab.inactiveBackground": "#d7a613",
    "tab.inactiveForeground": "#e3cbd5",
    "breadcrumb.foreground": "#c8fd0f",
    "breadcrumb.focusForeground": "#69742c",
    "breadcrumb.activeSelectionForeground": "#7ae5e9",

    "menu.background": "#07073bd3",
    "menu.foreground": "#71df72",
    "menu.selectionBackground": "#2bd328",
    "menu.selectionForeground": "#63eab8",
    "menu.selectionBorder": "#b7a317",
    "menu.border": "#cebbe9",

    "editorGroup.border": "#ffacec",
    "editorGroupHeader.tabsBackground": "#5e9a62",
    "editorGroup.dropBackground": "#FF174455",
    "editorGroup.dropIntoPromptBackground": "#612a95",
    "editorGroup.dropIntoPromptBorder": "#0d152a",
    "editorGroupHeader.border": "#1aac9a",

    "editorWidget.background": "#a4422c",
    "editorWidget.border": "#d7d7d7f6",
    "editorHoverWidget.background": "#04ea5d",
    "editorHoverWidget.border": "#44b4d9"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c32fd2"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#5d8171"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#a51c6f"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#39bd7f"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffffd4"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#1d805a"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "fontStyle": "",
        "foreground": "#a068c2"
      }
    },
    {
      "name": "Numbers, Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#a84783"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#bb5642"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#61c17e"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator entity.name",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "foreground": "#76a63a"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#11ca95"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#5f92c6"
      }
    },
    {
      "scope": ["constant.language.boolean.true"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#21cebe"
      }
    },
    {
      "scope": ["string.quoted.double"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#c091e2"
      }
    }
  ]
}
