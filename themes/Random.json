{
  "name": "Random",
  "type": "dark",
  "colors": {
    "editor.background": "#4713b2",
    "editor.foreground": "#ABC180",
    "activityBarBadge.background": "#27F2EA",
    "sideBarTitle.foreground": "#DDB110",
    "statusBar.background": "#0EE1D9",
    "statusBar.foreground": "#4CEF5F",
    "statusBar.border": "#02FD41",
    "statusBar.debuggingBackground": "#0CE550",
    "statusBar.debuggingForeground": "#65F5EA",
    "statusBar.noFolderBackground": "#D0EDE1",
    "statusBarItem.activeBackground": "#95D8BD",
    "statusBarItem.hoverBackground": "#34FDC1",
    "titleBar.activeBackground": "#74BEAC",
    "titleBar.activeForeground": "#F0C3E8",
    "titleBar.inactiveBackground": "#D9CA92",
    "titleBar.inactiveForeground": "#C99FEC",
    "titleBar.border": "#48B7E3",
    "activityBar.background": "#D7B15B",
    "activityBar.foreground": "#2FC0A3",
    "activityBar.border": "#84E76F",
    "sideBar.background": "#01EE9F",
    "sideBar.foreground": "#14FDE7",
    "sideBar.border": "#04D02B",
    "selection.background": "#FF174455",
    "editor.selectionBackground": "#ae354d33",
    "editor.wordHighlightBackground": "#a2181878",
    "editor.lineHighlightBackground": "#3c3b4e37",
    "editorCursor.foreground": "#3DC4BB",
    "editorLineNumber.foreground": "#ABE27D",
    "editorLineNumber.activeForeground": "#E4BE0C",
    "tab.activeBackground": "#E0CDBE",
    "tab.activeForeground": "#DEFB70",
    "tab.activeBorder": "#AEA9E3",
    "tab.inactiveBackground": "#C7E3DC",
    "tab.inactiveForeground": "#BAF7A5",
    "breadcrumb.foreground": "#AFB24D",
    "breadcrumb.focusForeground": "#35DD90",
    "breadcrumb.activeSelectionForeground": "#DEF7E3",

    "menu.background": "#07073bd3",
    "menu.foreground": "#67F4D2",
    "menu.selectionBackground": "#DBC33D",
    "menu.selectionForeground": "#8BF9A4",
    "menu.selectionBorder": "#D2FABA",
    "menu.border": "#F3C35B",

    "editorGroup.border": "#D5F492",
    "editorGroupHeader.tabsBackground": "#D3B6A5",
    "editorGroup.dropBackground": "#FF174455",
    "editorGroup.dropIntoPromptBackground": "#CEA4A6",
    "editorGroup.dropIntoPromptBorder": "#80C7B3",
    "editorGroupHeader.border": "#56CB43",

    "editorWidget.background": "#FB97A6",
    "editorWidget.border": "#d7d7d7f6",
    "editorHoverWidget.background": "#B1DEC0",
    "editorHoverWidget.border": "#FDBDFD"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B1DC15"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#D5EB9B"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#57CADF"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#0DFD29"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffffd4"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C0ED1C"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "fontStyle": "",
        "foreground": "#A1B68E"
      }
    },
    {
      "name": "Numbers, Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#F9F0B6"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#57FE81"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#95BB3B"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator entity.name",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "foreground": "#89B3D9"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#2FC7EB"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#58DC49"
      }
    },
    {
      "scope": ["constant.language.boolean.true"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#3FE814"
      }
    },
    {
      "scope": ["string.quoted.double"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#7CC8DA"
      }
    }
  ]
}
