{
  "name": "Random",
  "type": "dark",
  "colors": {
    "editor.background": "#4713b2",
    "editor.foreground": "#E1EC59",
    "activityBarBadge.background": "#78F3E7",
    "sideBarTitle.foreground": "#16F7B8",
    "statusBar.background": "#95EF59",
    "statusBar.foreground": "#FAF6F5",
    "statusBar.border": "#BFE4EE",
    "statusBar.debuggingBackground": "#A6DE98",
    "statusBar.debuggingForeground": "#F9B03C",
    "statusBar.noFolderBackground": "#CAFA3F",
    "statusBarItem.activeBackground": "#20F398",
    "statusBarItem.hoverBackground": "#95C522",
    "titleBar.activeBackground": "#1CC889",
    "titleBar.activeForeground": "#D3E6AE",
    "titleBar.inactiveBackground": "#C4F10A",
    "titleBar.inactiveForeground": "#3CE4F0",
    "titleBar.border": "#E0A678",
    "activityBar.background": "#5DDE6B",
    "activityBar.foreground": "#3AE57F",
    "activityBar.border": "#10EB75",
    "sideBar.background": "#B7FBAC",
    "sideBar.foreground": "#86DE2E",
    "sideBar.border": "#EE88E6",
    "selection.background": "#FF174455",
    "editor.selectionBackground": "#ae354d33",
    "editor.wordHighlightBackground": "#a2181878",
    "editor.lineHighlightBackground": "#3c3b4e37",
    "editorCursor.foreground": "#7EDE7A",
    "editorLineNumber.foreground": "#ACD04A",
    "editorLineNumber.activeForeground": "#8FBDB8",
    "tab.activeBackground": "#70E356",
    "tab.activeForeground": "#FE9DC8",
    "tab.activeBorder": "#29BEB8",
    "tab.inactiveBackground": "#D7A857",
    "tab.inactiveForeground": "#B6E63A",
    "breadcrumb.foreground": "#DEC4A8",
    "breadcrumb.focusForeground": "#54CC09",
    "breadcrumb.activeSelectionForeground": "#BCBB94",

    "menu.background": "#07073bd3",
    "menu.foreground": "#F2DD29",
    "menu.selectionBackground": "#FD900F",
    "menu.selectionForeground": "#D1AC95",
    "menu.selectionBorder": "#9EF1AD",
    "menu.border": "#A0E769",

    "editorGroup.border": "#8EDE7A",
    "editorGroupHeader.tabsBackground": "#69DD8C",
    "editorGroup.dropBackground": "#FF174455",
    "editorGroup.dropIntoPromptBackground": "#17EA92",
    "editorGroup.dropIntoPromptBorder": "#57E025",
    "editorGroupHeader.border": "#B6E098",

    "editorWidget.background": "#98B1D0",
    "editorWidget.border": "#d7d7d7f6",
    "editorHoverWidget.background": "#2ECCE6",
    "editorHoverWidget.border": "#5ED2C7"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4CE1E5"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#17D4FA"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#15C712"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#8FDC4B"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffffd4"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#63E770"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "fontStyle": "",
        "foreground": "#6FF68A"
      }
    },
    {
      "name": "Numbers, Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#92B903"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#F9D304"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#ADBCDA"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator entity.name",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "foreground": "#B1BC98"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#CAE4D1"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#99B1DD"
      }
    },
    {
      "scope": ["constant.language.boolean.true"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#09C4BF"
      }
    },
    {
      "scope": ["string.quoted.double"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#3BD1B6"
      }
    }
  ]
}
