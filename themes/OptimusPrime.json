{
  "name": "OptimusPrime",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1d3f",
    "editor.foreground": "#b08f8f",
    "activityBarBadge.background": "#b61e19",
    "sideBarTitle.foreground": "#7aa546",
    "statusBar.background": "#071175",
    "statusBar.foreground": "#E8F1FF",
    "statusBar.border": "#878f9b",
    "statusBar.debuggingBackground": "#FF0000",
    "statusBar.debuggingForeground": "#E8F1FF",
    "statusBar.noFolderBackground": "#ffffff",
    "statusBarItem.activeBackground": "#7b8a99",
    "statusBarItem.hoverBackground": "#15243b",
    "titleBar.activeBackground": "#010a53",
    "titleBar.activeForeground": "#7ea6fe",
    "titleBar.inactiveBackground": "#030a49",
    "titleBar.inactiveForeground": "#968b92",
    "titleBar.border": "#6767e6",
    "activityBar.background": "#060151",
    "activityBar.foreground": "#bcc1ce",
    "activityBar.border": "#6767e6",
    "sideBar.background": "#0e153c",
    "sideBar.foreground": "#6782be",
    "sideBar.border": "#0046b6",
    "selection.background": "#FF174455",
    "editor.selectionBackground": "#ae354d33",
    "editor.wordHighlightBackground": "#a2181878",
    "editor.lineHighlightBackground": "#3c3b4e37",
    "editorCursor.foreground": "#d7de11",
    "editorLineNumber.foreground": "#8B6D7B",
    "editorLineNumber.activeForeground": "#6767e6",
    "tab.activeBackground": "#060151",
    "tab.activeForeground": "#98aeff",
    "tab.activeBorder": "#b63030",
    "tab.inactiveBackground": "#0A2752",
    "tab.inactiveForeground": "#8B6D7B",
    "breadcrumb.foreground": "#dc6b29",
    "breadcrumb.focusForeground": "#020304",
    "breadcrumb.activeSelectionForeground": "#ae495d",

    "menu.background": "#07073bd3",
    "menu.foreground": "#ededed",
    "menu.selectionBackground": "#8e0216",
    "menu.selectionForeground": "#E8F1FF",
    "menu.selectionBorder": "#E8F1FF",
    "menu.border": "#4f2dcc",

    "editorGroup.border": "#244d8e",
    "editorGroupHeader.tabsBackground": "#0A2752",
    "editorGroup.dropBackground": "#FF174455",
    "editorGroup.dropIntoPromptBackground": "#657c9d",
    "editorGroup.dropIntoPromptBorder": "#E8F1FF",
    "editorGroupHeader.border": "#E8F1FF",

    "editorWidget.background": "#1d234a",
    "editorWidget.border": "#d7d7d7f6",
    "editorHoverWidget.background": "#2b135b",
    "editorHoverWidget.border": "#d8d8d8"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6272db"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#cbcbcb"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#afb1b4"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#f02222"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffffd4"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF4500"
      }
    },
    {
      "name": "String, Symbols",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "fontStyle": "",
        "foreground": "#e53d3d"
      }
    },
    {
      "name": "Numbers, Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#cb553a"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#2f69dd"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic  ",
        "foreground": "#94c8e6"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator entity.name",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "foreground": "#ce6177"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#73fbb5"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#e66109"
      }
    },
    {
      "scope": ["constant.language.boolean.true"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#09b6e6"
      }
    },
    {
      "scope": ["string.quoted.double"],
      "settings": {
        "fontStyle": "  italic",
        "foreground": "#289f95"
      }
    }
  ]
}
