    "titleBar.inactiveBackground": "#c7911b95",
    "titleBar.inactiveForeground": "#878787",
    "titleBar.border": "#f2f2f2",
    "activityBar.background": "#641114",
    "activityBar.foreground": "#ffe9be",
    "activityBar.border": "#ffffff",
    "sideBar.background": "#450e0e",
    "sideBar.foreground": "#fffad9c0",
    "sideBar.border": "#ffffff",
    "selection.background": "#FF174455",
    "editor.selectionBackground": "#ae354d33",
    "editor.wordHighlightBackground": "#a2181878",
    "editor.lineHighlightBackground": "#3c3b4e37",
    "editorCursor.foreground": "#d7de11",
    "editorLineNumber.foreground": "#8B6D7B",
    "editorLineNumber.activeForeground": "#fbd436",
    "tab.activeBackground": "#ceb37837",
    "tab.activeForeground": "#e5e5e5",
    "tab.activeBorder": "#ffffff",
    "tab.inactiveBackground": "#54210a",
    "tab.inactiveForeground": "#ff8d3b",
    "tab.border": "#ecec5550",
    "breadcrumb.foreground": "#a95829",
    "breadcrumb.focusForeground": "#020304",
    "breadcrumb.activeSelectionForeground": "#d49919",
    "menu.background": "#977835",
    "menu.foreground": "#ffffff",
    "menu.selectionBackground": "#8e0216",
    "menu.selectionForeground": "#E8F1FF",
    "menu.selectionBorder": "#E8F1FF",
    "editorGroup.border": "#eef4ff",
    "editorGroupHeader.tabsBackground": "#a2181878",
    "editorGroup.dropBackground": "#FF174455",
    "editorGroup.dropIntoPromptBackground": "#ebeef2",
    "editorGroup.dropIntoPromptBorder": "#E8F1FF",
    "editorGroupHeader.border": "#E8F1FF",

    "editorWidget.background": "#9c834e",
    "editorWidget.foreground": "#ffffff",
    "editorWidget.border": "#dd2d05",

    "editorHoverWidget.background": "#67542a",
    "editorHoverWidget.foreground": "#efefef",
    "editorHoverWidget.border": "#ffffff",
    "editorHoverWidget.statusBarBackground": "#463209",
    "editorHoverWidget.statusBarForeground": "#f94141"
  },
  "tokenColors": [
    {
      "scope": ["storage.type", "keyword.control"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f4f4f4"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#bd994c"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#dfcd84"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#dfcd84"
      }
    },
    {
      "scope": ["string.quoted"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c22020"
      }
    },
    {
      "scope": ["entity.name.function", "variable.language.special"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff7734"
      }
    },
    {
      "scope": ["string.quoted.single"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cdcaca"
      }
    },
    {
      "scope": ["comment.line.double-slash"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#adadad"
      }
    },
    {
      "scope": ["variable.other", "support.function.magic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fae34f"
      }
    },
    {
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#eea24a"
      }
    },
    {
      "scope": ["variable.parameter.function"],
      "settings": {
        "foreground": "#c3cd39"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cdaf3973"
      }
    }
  ]
}
