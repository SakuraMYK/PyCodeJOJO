{
  "name": "Frozen Crystal",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1b26e0",
    "editor.findMatchBackground": "#3d59a180",
    "editorWidget.background": "#1a1b26e0",
    "editor.foreground": "#E8F4F8",
    "activityBarBadge.background": "#A5D6F1",
    "sideBarTitle.foreground": "#88C3E8",
    "list.activeSelectionForeground": "#E8F4F8",
    "list.inactiveSelectionForeground": "#E8F4F8",
    "list.highlightForeground": "#A5D6F1",
    "scrollbarSlider.activeBackground": "#A5D6F180",
    "editorSuggestWidget.highlightForeground": "#A5D6F1",
    "textLink.foreground": "#A5D6F1",
    "progressBar.background": "#A5D6F1",
    "pickerGroup.foreground": "#A5D6F1",
    "tab.activeBorder": "#A5D6F1",
    "notificationLink.foreground": "#A5D6F1",
    "editorWidget.resizeBorder": "#A5D6F1",
    "editorWidget.border": "#A5D6F1",
    "settings.modifiedItemIndicator": "#A5D6F1",
    "settings.headerForeground": "#A5D6F1",
    "panelTitle.activeBorder": "#A5D6F1",
    "breadcrumb.activeSelectionForeground": "#E8F4F8",
    "menu.selectionForeground": "#E8F4F8",
    "menubar.selectionForeground": "#E8F4F8",
    "editor.findMatchBorder": "#A5D6F1",
    "selection.background": "#A5D6F140",
    "statusBarItem.remoteBackground": "#A5D6F1",
    "activityBar.background": "#1E2A3A",
    "titleBar.activeBackground": "#1E2A3A",
    "sideBar.background": "#182332",
    "statusBar.background": "#182332",
    "statusBar.noFolderBackground": "#182332",
    "statusBar.debuggingBackground": "#182332",
    "panel.background": "#182332",
    "terminal.background": "#182332",
    "tab.inactiveBackground": "#1E2A3A",
    "tab.activeBackground": "#243242",
    "editorGroupHeader.tabsBackground": "#1E2A3A",
    "dropdown.background": "#1E2A3A",
    "input.background": "#1E2A3A",
    "editor.lineHighlightBackground": "#A5D6F115",
    "editor.selectionBackground": "#88C3E840",
    "minimap.selectionHighlight": "#A5D6F180",
    "editor.findMatchHighlightBackground": "#88C3E830",
    "editorHoverWidget.background": "#1E2A3AE8",
    "editorSuggestWidget.background": "#1E2A3AE8",
    "peekViewEditor.background": "#1E2A3A",
    "peekView.border": "#A5D6F130",
    "peekViewResult.background": "#1E2A3A",
    "peekViewTitle.background": "#1E2A3A",
    "panel.border": "#A5D6F130",
    "statusBar.border": "#A5D6F130"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#565f89",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#E8F4F8"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#E8F4F8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#89ddff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#E8F4F8"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#B5E8E8"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#B5E8E8"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A5D6F1"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#88C3E8"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#B5A8E8"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#B5E8E8"
      }
    }
  ]
}
